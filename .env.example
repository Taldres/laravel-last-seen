# Example .env for Laravel Last Seen

# Enable or disable the last seen package (true/false)
LAST_SEEN_ENABLED=true

# User model class (optional, defaults to App\Models\User)
LAST_SEEN_USER_MODEL=App\Models\User

# Minimum seconds between last_seen updates
LAST_SEEN_UPDATE_THRESHOLD=60

# Seconds a user is considered recently seen after last activity
LAST_SEEN_RECENTLY_SEEN_THRESHOLD=300

